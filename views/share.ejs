<%- include('partials/header', { title: 'Shared list' }) %>

<h2 class="mb-3">Shared list: <%= list.title %></h2>

<% if (items.length === 0) { %>
  <p>No items on this list.</p>
<% } else { %>
  <ul class="list-group">
    <% items.forEach(item => { %>
      <li class="list-group-item"><%= item.item_name %></li>
    <% }) %>
  </ul>
<% } %>

<p class="mt-3">
  <a href="/">Back to app</a>
</p>

<%- include('partials/footer') %>
